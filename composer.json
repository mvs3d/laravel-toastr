{
    "name": "mvs3d/laravel-toastr",
    "description": "toastr.js for laravel 5.5",
    "keywords": [
        "toastr",
        "notification",
        "laravel",
        "php"
    ],
    "homepage": "https://github.com/mvs3d/laravel-toastr",
    "license": "MIT",
    "authors": [
        {
            "name": "whossun",
            "email": "whossun@gmail.com"
        },
        {
            "name": "Miguel Piedrafita",
            "email": "soy@miguelpiedrafita.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Whossun\\Toastr\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": ">=5.2.7",
        "illuminate/session": ">=5.2.7"
    },
    "extra":{
        "laravel":{
            "providers":[
                "Whossun\\Toastr\\ToastrServiceProvider"
            ],
            "aliases":{
                "Toastr": "Whossun\\Toastr\\Facades\\Toastr"
            }
        }
    }
}
